{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/share/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/dist/assets/$1"
    },
    {
      "src": "/sw.js",
      "dest": "client/dist/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "client/dist/manifest.json"
    },
    {
      "src": "/icon-192-192.png",
      "dest": "client/dist/icon-192-192.png"
    },
    {
      "src": "/icon-512-512.png",
      "dest": "client/dist/icon-512-512.png"
    },
    {
      "src": "/Nomad.svg",
      "dest": "client/dist/Nomad.svg"
    },
    {
      "src": "/Nomad.png",
      "dest": "client/dist/Nomad.png"
    },
    {
      "src": "/hyperwave-one.ttf",
      "dest": "client/dist/hyperwave-one.ttf"
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/index.html"
    }
  ]
}