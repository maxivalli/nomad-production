{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/share/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/assets/$1"
    },
    {
      "src": "/sw.js",
      "dest": "client/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "client/manifest.json"
    },
    {
      "src": "/manifest.webmanifest",
      "dest": "client/manifest.webmanifest"
    },
    {
      "src": "/icon-(.*)",
      "dest": "client/icon-$1"
    },
    {
      "src": "/badge-(.*)",
      "dest": "client/badge-$1"
    },
    {
      "src": "/Nomad.(.*)",
      "dest": "client/Nomad.$1"
    },
    {
      "src": "/hyperwave-one.ttf",
      "dest": "client/hyperwave-one.ttf"
    },
    {
      "src": "/registerSW.js",
      "dest": "client/registerSW.js"
    },
    {
      "src": "/robots.txt",
      "dest": "client/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "client/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}